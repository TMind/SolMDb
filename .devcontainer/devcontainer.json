{  
    "name": "MongoDB Environment",  
    "dockerFile": "Dockerfile",    
    "service": "jupyter",
    "settings": {  
        "terminal.integrated.shell.linux": "/bin/bash"  
    },
    "forwardPorts": [27017, 8081, 8888],  
    "extensions": [  
        "mongodb.mongodb-vscode",  
        "ms-toolsai.jupyter",  
        "ms-python.python"  
    ],  
    "runArgs": ["--init"],

    "postCreateCommand": "/usr/local/bin/post-create.sh"  

}  
